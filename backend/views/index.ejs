<!DOCTYPE html>
<html lang="en">
  <head>
    <meta charset="UTF-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title><%= data.title %></title>
    <link
      rel="stylesheet"
      href="https://cdn.jsdelivr.net/npm/swiper@10/swiper-bundle.min.css"
    />
    <link rel="preconnect" href="https://fonts.googleapis.com" />
    <link rel="preconnect" href="https://fonts.gstatic.com" crossorigin />
    <link
      href="https://fonts.googleapis.com/css2?family=Bodoni+Moda:wght@600&family=Inter&family=Oswald:wght@600&family=Roboto&display=swap"
      rel="stylesheet"
    />
    <link
      href="https://cdn.jsdelivr.net/npm/bootstrap@5.0.2/dist/css/bootstrap.min.css"
      rel="stylesheet"
      integrity="sha384-EVSTQN3/azprG1Anm3QDgpJLIm9Nao0Yz1ztcQTwFspd3yD65VohhpuuCOmLASjC"
      crossorigin="anonymous"
    />
    <link rel="stylesheet" href="/css/index.css" />
  </head>
  <body>
    <div class="_container">
      <div class="burger__menu">
        <ul>
          <a href="/portfolio">Portefeuille</a>
          <li class="btnPrice">Prix</li>
          <li class="btnContact">Contact</li>
        </ul>
      </div>

      <header class="header">
        <div class="header__logo">
          <h1 class="header__logo_h1">IRINA ADAMENKO</h1>
          <p class="header__logo_p">photography</p>
        </div>

        <div class="header__menu">
          <ul>
            <a href="/portfolio">Portefeuille</a>
            <li class="btnPrice">Prix</li>
            <li class="btnContact">Contact</li>
          </ul>
        </div>
        <div class="burger-container">
          <div class="header__burger">
            <span class="header__burger_long"></span>
            <span class="header__burger_long"></span>
            <span class="header__burger_long"></span>
          </div>
        </div>
      </header>

      <!-- Slider main container -->
      <div class="swiper">
        <!-- Additional required wrapper -->
        <div class="swiper-wrapper">
          <!-- Slides -->
          <% for (let i = 0; i < data.swiperIndex.length; i++) { %>
          <div class="swiper-slide">
            <div class="super__image" style="background-image: url('<%= data.swiperIndex[i]%>');"></div>
            
          </div>
          <% } %>
        </div>
        <!-- <button class="swiper-btn">Portfolio</button> -->
      </div>

      <main class="main">
        <div class="main__aboutMe">
          <div class="main__aboutMe_containerPhoto">
            <img src="<%= data.textAboutMe.photo %>" alt="" />
          </div>
          <div class="main__aboutMe_text">
            <h3 class="text_title">Bonjour et bienvenue !</h3>
            <% for (i = 0; i < data.textAboutMe.text.length; i++) { %>
            <p class="text">
              <%=  data.textAboutMe.text[i] %>
            </p>
            <% } %>
          </div>
        </div>

        <div class="work-style">
          <div class="work-style_head">
            <h1 class="work-style_name">Mes prestations</h1>
          </div>

          
          <div class="work-style__items">
            <% for(let i = 0; i < data.workStyle.length; i++){ %>
            <div class="work-style_container">
              <h1 class="container-name"><%= data.workStyle[i].name %></h1>
              <img src="<%= data.workStyle[i].img %>" alt="" />
              <div class="container-price"><%= data.workStyle[i].price %></div>
              <h3 class="container-name2"><%= data.workStyle[i].form %></h2>
              <div class="container-info">
                <ul>
                  <% for (let j = 0; j < data.workStyle[i].info.length; j++) { %>
                    <li><%= data.workStyle[i].info[j] %></li>
                    <% } %>
                </ul>
              </div>
            </div>
            <% } %>
              </div>
              
            </div>
          </div>
        </div>
      </main>
    </div>

    <footer>
      <div class="footer">
        <div class="footer__icons">
          <%for(let i = 0; i < data.contact.length; i++){ %>
          <a href="<%= data.contact[i].url%>">
            <div class="footer__icons_icon">
              <img src="<%= data.contact[i].icon%>" alt="" />
            </div>
          </a>
          <% } %>
          
        </div>

        <!-- <a class="footer__number" href="tel:+3806823222280">+3806823222280</a> -->

        <div class="footer__email"><%= data.email %></div>
        <div class="footer__creator">Â© 2023 Anton Mokriy</div>
      </div>
    </footer>

 
    <script src="https://cdn.jsdelivr.net/npm/swiper@10/swiper-bundle.min.js"></script>
    <script src="/js/indexJs.js"></script>
    
  </body>
</html>
